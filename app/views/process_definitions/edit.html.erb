<%= title [l(:bpm_processes), bpm_integration_process_definitions_path], @process_definition.name %>

<%= form_for @process_definition, url: {action: "update", class: 'bpms'} do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>
<%= render :partial => 'process_definition_versions/list', locals: { process_definition_versions: @process_definition.versions.order('version desc') } %>
<%= submit_tag t(:button_save) %>
<% end %>
